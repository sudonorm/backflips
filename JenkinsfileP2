pipeline{
    agent any
    
   // triggers {
        //cron('H */3 * * *') // Run script everyday at 8:53am 
        //upstream(upstreamProjects: 'mainpipeline', threshold: hudson.model.Result.SUCCESS)
    //}
    stages {
        stage('Checkout Code') {
            steps {
                bat 'echo "code checked out in pipeline 2" ' // this stage can be used to set-up some git checkout if "pipeline with SCM" is not selected while setting up the job
            }
        }

        
    }
 
    post {
        always {
                cleanWs() // clean up workspace
            }
	    }
}